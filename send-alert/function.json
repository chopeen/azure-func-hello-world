{
  "disabled": false,
  "bindings": [
    {
      "authLevel": "function",
      "name": "req",
      "type": "httpTrigger",
      "direction": "in",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "name": "$return",
      "type": "http",
      "direction": "out"
    },
    {
      "name": "message",
      "type": "sendGrid",
      "direction": "out",
      "apiKey" : "SendGridKey",
      "to": "EmailToAlert",
      "from": "EmailFromAlert",
      "subject": "[not-mine-alert] Check the scrobbles!"
    }
  ]
}